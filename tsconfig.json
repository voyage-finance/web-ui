{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./src",
    "paths": {
      "@abi/*": ["abi/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants": ["consts"],
      "@hooks/*": ["hooks/*"],
      "@graph": ["../.graphclient/index.ts"],
      "@utils/*": ["utils/*"],
      "@queries/*": ["graphql/queries/*"],
      "@types": ["types"]
    }
  },
  "include": ["next-env.d.ts", "src/typings/*.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
